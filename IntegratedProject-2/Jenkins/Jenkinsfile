pipeline {
    agent any
    environment {
		name="NAME"
    }
    stages {
        stage("Checkout Code") {
            steps {
                checkout scm
            }
        }
        stage("Build Docker Image") {
            steps {
				dir('IntegratedProject-2'){
				sh "pwd"
				sh "ls -la"
                sh "docker build -t writetoshell:${BUILD_NUMBER} ."
				}
            }
        }
    }
}